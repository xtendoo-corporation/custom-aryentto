<odoo noupdate="1">
    <!-- Sequence for barcode generation -->
    <record id="seq_sale_quotation" model="ir.sequence">
        <field name="name">Product Product Barcode Sequence</field>
        <field name="code">product.product.barcode.sequence</field>
        <field name="prefix">AR/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False" />
    </record>
</odoo>
